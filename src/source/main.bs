sub main(args as roAssociativeArray)
    m.verbose = true
    m.timing = true

    timeSpan as roTimeSpan = invalid

    if m.timing
        timeSpan = CreateObject("roTimeSpan")
    end if

    result = day4.part2("04.txt")
    print result

    if m.timing
        print `Total Time: ${timeSpan.TotalMilliseconds()}ms`
    end if

end sub
